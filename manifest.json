{
  "manifest_version": 2,

  "name": "The Agenda",

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+X",
        "mac": "Command+Shift+X",
        "chromeos": "Ctrl+Shift+X",
        "linux": "Ctrl+Shift+X"
      }
    }

  },
  "description": "Quick and easy time management",
  "version": "1.4.4.15",
  "icons": { "16": "Agenda_16.png",
             "32": "Agenda_32.png",
             "48": "Agenda_48.png",
            "128": "Agenda_128.png"},

  "browser_action": {
    "default_icon": "Agenda_32.png",
    "default_popup": "mainWindow.html"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },


  "permissions": ["tabs", "notifications", "alarms", "<all_urls>"]
}
